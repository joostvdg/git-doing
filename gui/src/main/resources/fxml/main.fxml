<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.github.joostvdg.gitdoing.gui.controller.MainController"
            stylesheets="@../css/bootstrap3.css">
   <children>
       <VBox fx:id="dataContainer"
             AnchorPane.leftAnchor="10.0"
             AnchorPane.rightAnchor="10.0"
             AnchorPane.topAnchor="50.0">
       </VBox>
      <MenuBar prefHeight="25.0" prefWidth="1280.0">
        <menus>
          <Menu mnemonicParsing="false" text="File" fx:id="fileMenu">
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
